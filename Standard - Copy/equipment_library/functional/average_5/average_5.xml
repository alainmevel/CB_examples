else {
&#9;switch(nbInputs) {
&#9;&#9;case 2 : result = input1 + input2;
&#9;&#9;&#9;break;
&#9;&#9;case 3 : result = input1 + input2 + input3;
&#9;&#9;&#9;break;
&#9;&#9;case 4 : result = input1 + input2 + input3 + input4;
&#9;&#9;&#9;break;
&#9;&#9;case 5 : result = input1 + input2 + input3 + input4 + input5; 
&#9;&#9;&#9;break;
&#9;&#9;default : result = 0.0;
&#9;&#9;
&#9;&#9;&#9;break;
&#9;}

&#9;if (nbInputs &gt; 0) result = result / nbInputs;
}
